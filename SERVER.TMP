reverse_shift() {
    local input="$1"
    local result=""
    local i=0

    while [ $i -lt ${#input} ]; do
        if [[ "$i" -le $(( ${#input} - 7 )) && "${input:$i:7}" == "PGYNKPG" ]]; then
            result+=$'\n'
            i=$((i + 7))
        else
            char="${input:$i:1}"
            shifted_char=$(printf "\\$(printf '%03o' $(( $(printf '%d' "'$char") - 2 )))")
            result+="$shifted_char"
            ((i++))
        fi
    done

    echo "$result"
}
